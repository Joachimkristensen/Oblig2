@using Oblig2.Models.Entities
@model IEnumerable<Oblig2.Models.Entities.BlogApplicationUser>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a>Your subscriptions</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Blog.BlogId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Blog.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Blog.CreationDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Blog.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Blog.Description)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Blog.BlogId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Blog.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Blog.CreationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Blog.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Blog.Description)
            </td>
        </tr>
}
    </tbody>
</table>
